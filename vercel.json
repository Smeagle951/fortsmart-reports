{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "installCommand": "npm install --legacy-peer-deps",
    "framework": "nextjs",
    "regions": [
        "gru1"
    ],
    "env": {
        "FORTSMART_DB_PATH": ""
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, must-revalidate"
                }
            ]
        }
    ]
}